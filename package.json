{
  "name": "http-range-fetcher",
  "version": "1.2.5",
  "description": "caching, aggregating fetch manager for doing lots of HTTP range requests",
  "license": "MIT",
  "repository": "rbuels/http-range-fetcher",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "author": {
    "name": "Robert Buels",
    "email": "rbuels@gmail.com",
    "url": "https://github.com/rbuels"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "lint": "eslint src test",
    "docs": "documentation readme --shallow src/HttpRangeFetcher.js --section=API",
    "clean": "rimraf dist esm",
    "build:esm": "tsc --target es2018 --outDir esm",
    "build:es5": "tsc --target es5 --outDir dist",
    "build": "npm run build:esm && npm run build:es5",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "postversion": "git push --follow-tags",
    "version": "standard-changelog && git add CHANGELOG.md"
  },
  "keywords": [],
  "dependencies": {
    "abortcontroller-polyfill": "^1.2.9",
    "cross-fetch": "^2.2.2",
    "object.entries-ponyfill": "^1.0.1",
    "quick-lru": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "documentation": "^6.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "lodash": "^4.17.10",
    "prettier": "^2.5.1",
    "rimraf": "^2.6.2",
    "standard-changelog": "^1.0.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
