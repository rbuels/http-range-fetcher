{
  "name": "http-range-fetcher",
  "version": "1.8.0",
  "description": "caching, aggregating fetch manager for doing lots of HTTP range requests",
  "license": "MIT",
  "repository": "rbuels/http-range-fetcher",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "author": {
    "name": "Robert Buels",
    "email": "rbuels@gmail.com",
    "url": "https://github.com/rbuels"
  },
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "scripts": {
    "test": "vitest",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "docs": "documentation readme --shallow src/HttpRangeFetcher.js --section=API",
    "clean": "rimraf dist esm",
    "build:esm": "tsc --target es2018 --outDir esm",
    "build:es5": "tsc --target es2015 --module commonjs --outDir dist",
    "build": "npm run build:esm && npm run build:es5",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run lint && npm test run && npm run build",
    "postversion": "git push --follow-tags",
    "version": "standard-changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "quick-lru": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "documentation": "^14.0.2",
    "eslint": "^9.9.0",
    "prettier": "^3.3.2",
    "rimraf": "^6.0.1",
    "standard-changelog": "^6.0.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.21.0",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
