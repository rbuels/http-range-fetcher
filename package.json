{
  "name": "http-range-fetcher",
  "version": "1.5.0",
  "description": "caching, aggregating fetch manager for doing lots of HTTP range requests",
  "license": "MIT",
  "repository": "rbuels/http-range-fetcher",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "author": {
    "name": "Robert Buels",
    "email": "rbuels@gmail.com",
    "url": "https://github.com/rbuels"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "lint": "eslint src test",
    "docs": "documentation readme --shallow src/HttpRangeFetcher.js --section=API",
    "clean": "rimraf dist esm",
    "build:esm": "tsc --target es2018 --outDir esm",
    "build:es5": "tsc --target es5 --outDir dist",
    "build": "npm run build:esm && npm run build:es5",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "postversion": "git push --follow-tags",
    "version": "standard-changelog && git add CHANGELOG.md"
  },
  "keywords": [],
  "dependencies": {
    "abortcontroller-polyfill": "^1.2.9",
    "node-fetch": "^2.6.7",
    "object.entries-ponyfill": "^1.0.1",
    "quick-lru": "^2.0.0",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "documentation": "^13.2.5",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "standard-changelog": "^2.0.27",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
